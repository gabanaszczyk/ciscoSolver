{
  "manifest_version": 3,
  "name": "ciscoSolver",
  "version": "1.0",
  "description": "Simple Chrome extension for Cisco Netacad Quizzes.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["answers.json"],
      "matches": ["<all_urls>"]
    }
  ]
}
